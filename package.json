{\n  \"name\": \"smaregi-mcp\",\n  \"version\": \"1.0.0\",\n  \"description\": \"Microservice Connection Point for integrating Smaregi POS data with Claude AI\",\n  \"type\": \"module\",\n  \"scripts\": {\n    \"pre_build\": \"npm run generate:api:common && npm run generate:api:pos && npm run convert && npm run generate:zod && npm run generate:typed-openapi && npm run generate:api:endpoints && npm run generate:api:tools\",\n    \"build\": \"tsc\",\n    \"format\": \"prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"\",\n    \"start\": \"nest start\",\n    \"start:dev\": \"nest start --watch\",\n    \"start:debug\": \"nest start --debug --watch\",\n    \"start:prod\": \"node dist/main.js\",\n    \"lint\": \"eslint \\\"{src,apps,libs,test}/**/*.ts\\\" --fix\",\n    \"test\": \"jest\",\n    \"test:watch\": \"jest --watch\",\n    \"test:cov\": \"jest --coverage\",\n    \"test:debug\": \"node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand\",\n    \"test:e2e\": \"jest --config ./test/jest-e2e.json\",\n    \"test:exit\": \"node tests/process-exit.test.js\",\n    \"generate:api:common\": \" npx openapi-typescript ./openapi/common/openapi.yaml -o ./src/schema/common.d.ts\",\n    \"generate:api:pos\": \" npx openapi-typescript ./openapi/pos/openapi.yaml -o ./src/schema/pos.d.ts\",\n    \"convert\": \"npx tsx ./scripts/generate-api-schema-json.ts\",\n    \"generate:zod\": \"npx tsx ./scripts/generate-zod-schemas.ts\",\n    \"generate:typed-openapi\": \"npx typed-openapi ./openapi/pos/openapi.yaml -r zod --output ./src/schema/zod/pos.zod.ts\",\n    \"generate:api:endpoints\": \"npx tsx ./scripts/generate-api-endpoints.ts\",\n    \"generate:api:tools\": \"npx tsx ./scripts/generate-api-tools-json.ts\",\n    \"mcp:init\": \"cp -r ./src/tools/generated ./dist/tools/ && node dist/main.js init\",\n    \"mcp:run\": \"node dist/main.js run\",\n    \"mcp:help\": \"node dist/main.js\",\n    \"mcp:dev\": \"nest start --watch\",\n    \"mcp:inspect\": \"npx @modelcontextprotocol/inspector node dist/main.js run\"\n  },\n  \"keywords\": [\n    \"smaregi\",\n    \"claude\",\n    \"api\",\n    \"pos\",\n    \"integration\",\n    \"mcp\"\n  ],\n  \"author\": \"\",\n  \"license\": \"ISC\",\n  \"dependencies\": {\n    \"@modelcontextprotocol/sdk\": \"^1.8.0\",\n    \"@nestjs/axios\": \"^4.0.0\",\n    \"@nestjs/common\": \"^11.0.12\",\n    \"@nestjs/config\": \"^4.0.2\",\n    \"@nestjs/core\": \"^11.0.12\",\n    \"@nestjs/platform-express\": \"^11.0.12\",\n    \"@nestjs/typeorm\": \"^11.0.0\",\n    \"axios\": \"^1.8.4\",\n    \"chalk\": \"^5.3.0\",\n    \"class-transformer\": \"^0.5.1\",\n    \"class-validator\": \"^0.14.1\",\n    \"dotenv\": \"^16.4.5\",\n    \"js-yaml\": \"^4.1.0\",\n    \"openapi-fetch\": \"^0.13.5\",\n    \"openid-client\": \"^6.4.1\",\n    \"reflect-metadata\": \"^0.2.2\",\n    \"sqlite3\": \"^5.1.7\",\n    \"typeorm\": \"^0.3.21\",\n    \"uuid\": \"^11.1.0\",\n    \"zod\": \"^3.24.2\"\n  },\n  \"devDependencies\": {\n    \"@apidevtools/swagger-cli\": \"^4.0.4\",\n    \"@eslint/js\": \"^9.22.0\",\n    \"@modelcontextprotocol/inspector\": \"^0.8.1\",\n    \"@nestjs/cli\": \"^11.0.5\",\n    \"@nestjs/schematics\": \"^11.0.2\",\n    \"@redocly/cli\": \"^1.34.1\",\n    \"@types/express\": \"^4.17.17\",\n    \"@types/jest\": \"^29.5.2\",\n    \"@types/js-yaml\": \"^4.0.9\",\n    \"@types/node\": \"^20.10.0\",\n    \"@types/supertest\": \"^2.0.12\",\n    \"@types/uuid\": \"^9.0.1\",\n    \"eslint\": \"^8.57.0\",\n    \"jest\": \"^29.5.0\",\n    \"js-yaml\": \"^4.1.0\",\n    \"openapi-typescript\": \"^7.6.1\",\n    \"playwright\": \"^1.51.1\",\n    \"prettier\": \"3.5.3\",\n    \"supertest\": \"^6.3.3\",\n    \"ts-jest\": \"^29.1.0\",\n    \"ts-loader\": \"^9.4.3\",\n    \"ts-node\": \"^10.9.2\",\n    \"ts-to-zod\": \"^3.15.0\",\n    \"tsconfig-paths\": \"^4.2.0\",\n    \"typed-openapi\": \"^0.10.1\",\n    \"typescript\": \"^5.0.0\",\n    \"typescript-eslint\": \"^8.26.1\",\n    \"typescript-json-schema\": \"^0.65.1\"\n  },\n  \"jest\": {\n    \"moduleFileExtensions\": [\n      \"js\",\n      \"json\",\n      \"ts\"\n    ],\n    \"rootDir\": \"src\",\n    \"testRegex\": \".*\\\\.spec\\\\.ts$\",\n    \"transform\": {\n      \"^.+\\\\.(t|j)s$\": \"ts-jest\"\n    },\n    \"collectCoverageFrom\": [\n      \"**/*.(t|j)s\"\n    ],\n    \"coverageDirectory\": \"../coverage\",\n    \"testEnvironment\": \"node\"\n  }\n}\n